{
  "manifest_version": 3,
  "name": "Aether Companion",
  "version": "0.1.0",
  "description": "Interact with the Aether platform directly from your cloud provider consoles.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
